{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/ask",
      "dest": "api.py"
    },
    {
      "src": "/index.js",
      "dest": "index.js"
    },
    {
      "src": "/index.css",
      "dest": "index.css"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}